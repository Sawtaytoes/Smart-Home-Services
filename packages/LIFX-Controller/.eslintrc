{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "no-console": 0,
    "no-unused-vars": ["error", {
      "argsIgnorePattern": "^_*$",
      "varsIgnorePattern": "^_*$"
    }],
    "strict": [2, "never"]
  }
}
