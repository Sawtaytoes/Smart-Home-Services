{
  "name": "@smart-home-services/flic-controller",
  "version": "3.0.6",
  "description": "Flic controller software using WebSockets.",
  "license": "MITNFA",
  "scripts": {
    "debug": "nodemon --inspect app.js",
    "lint": "node app.js lint",
    "lint:watch": "nodemon app.js lint",
    "start": "nodemon app.js",
    "test": "ava",
    "test:watch": "ava --watch"
  },
  "engines": {
    "node": ">=8.11.4",
    "yarn": ">=1.6.0"
  },
  "_moduleAliases": {
    "$redux": "./redux",
    "$utils": "./utils"
  },
  "ava": {
    "babel": {
      "testOptions": {
        "babelrc": false
      }
    },
    "concurrency": 5,
    "files": [
      "redux/**/*.test.js",
      "utils/**/*.test.js"
    ],
    "sources": [
      "**/*.js"
    ]
  },
  "dependencies": {
    "@redux-observable-backend/core": "^4.4.0",
    "@redux-observable-backend/node": "^4.4.0",
    "@redux-observable-backend/redux-utils": "^4.4.0",
    "@redux-observable-backend/websocket": "^4.4.0",
    "better-module-alias": "^1.1.0",
    "chalk": "^4.1.0",
    "fliclib": "git+https://github.com/50ButtonsEach/fliclib-linux-hci.git#73a8acb1531ad1b33480b4234ffff0a290d8a1aa",
    "node-fetch": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Sawtaytoes/Smart-Home-Services.git"
  }
}
